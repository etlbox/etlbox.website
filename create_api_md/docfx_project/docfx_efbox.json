{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\..\\..\\etlbox.source\\EFBox",
          "properties": {
            "TargetFramework": "netstandard2.0"
          }
        },
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\..\\..\\etlbox.source\\EFBox.MySql",
          "properties": {
            "TargetFramework": "netstandard2.0"
          }
        },
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\..\\..\\etlbox.source\\EFBox.Postgres",
          "properties": {
            "TargetFramework": "netstandard2.0"
          }
        },
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\..\\..\\etlbox.source\\ETLBox.SqlServer",
          "properties": {
            "TargetFramework": "netstandard2.0"
          }
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      }
    ],
    "dest": "_site_efbox",
    "template": [
      "statictoc",
      "templates"
    ],
    "maxParallelism" : 1,
    "globalMetadata": {
      "_disableFooter": true,
      "_disableContribution": true,
      "_disableNavbar": true,
      "_disableBreadcrumb": true,
      "_disableToc": true,
      "_disableAffix": true,
      "_enableSearch": false
    },
    "markdownEngineName": "markdig"
  }
}